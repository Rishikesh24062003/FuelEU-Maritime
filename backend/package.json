{
  "name": "fueleu-maritime-backend",
  "version": "1.0.0",
  "description": "FuelEU Maritime Compliance System - Backend",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest --coverage --runInBand",
    "test:watch": "jest --watch",
    "test:unit": "jest tests/unit",
    "test:integration": "NODE_ENV=test jest tests/integration --runInBand",
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/adapters/inbound/http/server.ts",
    "start": "node dist/adapters/inbound/http/server.js",
    "migrate": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "seed": "ts-node prisma/seed.ts",
    "prisma:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:reset": "prisma migrate reset --force",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\""
  },
  "keywords": [
    "fueleu",
    "maritime",
    "compliance",
    "ghg",
    "emissions"
  ],
  "author": "",
  "license": "MIT",
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "prisma": "^5.0.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@prisma/client": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "zod": "^3.22.4"
  }
}
